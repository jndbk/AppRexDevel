<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape
			android:shape="rectangle"
			android:id="@+id/rectangle" >
			
			<padding
				android:left="30dip"
				android:right="30dip"
				android:bottom="0dip"
				android:top="0dip">
			</padding>
			
			<stroke
				android:bottom="0dip"
				android:left="1dip"
				android:right="5dip"
				android:top="1dip"
				android:color="#FF000000" />
			<solid 
				android:color="#FFFFFFFF" />
		</shape>
	</item>
</layer-list>